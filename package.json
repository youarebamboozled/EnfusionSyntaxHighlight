{
  "name": "enfusion-script-syntax-highlighting",
  "displayName": "Enfusion Script Syntax Highlighting",
  "description": "Simple syntax highlighting for Enfusion Script files.",
  "version": "0.0.1",
  "publisher": "YouAreBamboozled",
  "license": "MIT",
  "author": {
    "name": "YouAreBamboozled",
    "email": "mojo@youarebamboozled.com"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "enfusion",
      "aliases": ["Enfusion Script", "enfusion"],
      "extensions": [".c"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "enfusion",
      "scopeName": "source.enfusion",
      "path": "./syntaxes/enfusion.tmLanguage.json"
    }],
    "textmateThemedPaths": [
      "./lightgreen-highlight.css"
    ]
  }
}
