{
  "name": "enforce-script-syntax-highlighting",
  "displayName": "Enforce Script Syntax Highlighting",
  "description": "Simple syntax highlighting for Enforce Script files.",
  "version": "0.0.6",
  "publisher": "YouAreBamboozled",
  "license": "MIT",
  "author": {
    "name": "YouAreBamboozled",
    "email": "mojo@youarebamboozled.com"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "enforce",
      "aliases": ["Enforce Script", "enforce"],
      "extensions": [".c"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "enforce",
      "scopeName": "source.enforce",
      "path": "./syntaxes/enforce.tmLanguage.json"
    }],
    "textmateThemedPaths": [
      "./lightgreen-highlight.css"
    ]
  }
}
